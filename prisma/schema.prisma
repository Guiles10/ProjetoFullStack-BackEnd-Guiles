// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String @id
  name  String
  email     String @unique
  telefone  String
  password  String
  createdAt String

  contacts Contact[]

  @@map("users")
}

model Contact {
  id        String @id
  name      String
  email     String @unique
  telefone  String
  createdAt String

  userId    String
  user User @relation(fields: [userId], references: [id])


  @@map("contacts")
}